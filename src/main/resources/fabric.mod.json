{
  "schemaVersion": 1,
  "id": "meteor-wheel",
  "version": "${version}",
  "name": "Wheel",
  "description": "An addon for Meteor Client.",
  "authors": [
    "Twoten_"
  ],
  "contact": {
    "repo": "https://github.com/tw0ten/mc-meteor-wheel"
  },
  "license": "GPL-3.0",
  "icon": "assets/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "twoten.meteor.wheel.Addon"
    ]
  },
  "mixins": [
    "meteor-wheel.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25",
    "modmenu": {
      "parent": {
        "id": "meteor-client"
      }
    }
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
